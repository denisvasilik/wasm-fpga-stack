    WRITE_RAM 32 #x0C #xAABBCCDD -- LowValue
    WRITE_RAM 32 #x0D #x00000000 -- HighValue
    WRITE_RAM 32 #x0E $STACKBLK_VAL_i32 -- TypeValue

    -- Current Activation Frame
    WRITE_RAM 32 #x0F #xFFFFFFFF -- Module instance UID
    WRITE_RAM 32 #x10 #xFFFFFFFF -- Return address
    WRITE_RAM 32 #x11 #x00000000 -- Previous Activation Frame Address
    WRITE_RAM 32 #x12 #x00000003 -- Max locals
    WRITE_RAM 32 #x13 #x00000001 -- Max results
    WRITE_RAM 32 #x14 #x00000005 -- Type: Activation Frame

    -- First Value on Stack
    WRITE_RAM 32 #x15 #xAABBCCDD -- LowValue
    WRITE_RAM 32 #x16 #x00000000 -- HighValue
    WRITE_RAM 32 #x17 $STACKBLK_VAL_i32 -- TypeValue

    -- Set activaten frame address
    WRITE_FPGA 32 $STACKBLK_ADR_ActivationFrameAddressReg #x0F

    -- Set stack address after current activation frame + 1 stack value
    WRITE_FPGA 32 $STACKBLK_ADR_StackAddressReg #x18

    EQU_VAR WB_VALUE $STACKBLK_VAL_DoRun
    OR_VAR WB_VALUE $STACKBLK_VAL_RemoveActivationFrame
    EQU_VAR WB_EXPECTED_VALUE $WB_VALUE
    WRITE_FPGA 32 $STACKBLK_ADR_ControlReg $WB_VALUE
    VERIFY_FPGA 32 $STACKBLK_ADR_ControlReg WB_VALUE $WB_EXPECTED_VALUE #xFFFFFFFF

    -- Wait until operation has been finished
    WAIT_NS 1000

    -- Previous activaten frame address
    VERIFY_FPGA 32 $STACKBLK_ADR_ActivationFrameAddressReg WB_VALUE #x0 #xFFFFFFFF

    -- Stack address after current activation frame has been removed
    VERIFY_FPGA 32 $STACKBLK_ADR_StackAddressReg WB_VALUE #x9 #xFFFFFFFF

    -- Frame information provided to registers
    VERIFY_FPGA 32 $STACKBLK_ADR_MaxLocalsReg WB_VALUE #x00000003 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_MaxResultsReg WB_VALUE #x00000001 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_ReturnAddressReg WB_VALUE #xFFFFFFFF #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_ModuleInstanceUidReg WB_VALUE #xFFFFFFFF #xFFFFFFFF

    -- Result provided to registers
    VERIFY_FPGA 32 $STACKBLK_ADR_LowValueReg WB_VALUE #xAABBCCDD #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_HighValueReg WB_VALUE #x00000000 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_TypeReg WB_VALUE $STACKBLK_VAL_i32 #xFFFFFFFF

    RETURN_CALL
END_SUB


