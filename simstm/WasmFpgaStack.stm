TEST_LOCAL_GET_BIT:
BEGIN_SUB
    MESSAGE 0 "TEST_LOCAL_GET_BIT"

    -- Previous Activation Frame
    WRITE_RAM 32 #x00 #xFFFFFFFF -- Module instance UID
    WRITE_RAM 32 #x01 #xFFFFFFFF -- Return address
    WRITE_RAM 32 #x02 #x00000005 -- Type: Activation Frame
    WRITE_RAM 32 #x03 #x00000000 -- Max locals
    WRITE_RAM 32 #x04 #x00000000 -- Max results
    WRITE_RAM 32 #x05 #x00000005 -- Type: Activation Frame

    -- Local 2
    WRITE_RAM 32 #x06 #x11223344 -- LowValue
    WRITE_RAM 32 #x07 #x00000000 -- HighValue
    WRITE_RAM 32 #x08 $STACKBLK_VAL_i32 -- TypeValue
    -- Local 1
    WRITE_RAM 32 #x09 #x44556677 -- LowValue
    WRITE_RAM 32 #x0A #x00000000 -- HighValue
    WRITE_RAM 32 #x0B $STACKBLK_VAL_i32 -- TypeValue
    -- Local 0
    WRITE_RAM 32 #x0C #xAABBCCDD -- LowValue
    WRITE_RAM 32 #x0D #x00000000 -- HighValue
    WRITE_RAM 32 #x0E $STACKBLK_VAL_i32 -- TypeValue

    -- Current Activation Frame
    WRITE_RAM 32 #x0F #xFFFFFFFF -- Module instance UID
    WRITE_RAM 32 #x10 #xFFFFFFFF -- Return address
    WRITE_RAM 32 #x11 #x00000005 -- Type: Activation Frame
    WRITE_RAM 32 #x12 #x00000003 -- Max locals
    WRITE_RAM 32 #x13 #x00000000 -- Max results
    WRITE_RAM 32 #x14 #x00000005 -- Type: Activation Frame

    -- Set activaten frame address
    WRITE_FPGA 32 $STACKBLK_ADR_ActivationFrameAddressReg #x0F

    -- Set stack address after current activation frame
    WRITE_FPGA 32 $STACKBLK_ADR_StackAddressReg #x15

    EQU_VAR WB_VALUE $STACKBLK_VAL_DoRun
    OR_VAR WB_VALUE $STACKBLK_VAL_LocalGet
    EQU_VAR WB_EXPECTED_VALUE $WB_VALUE
    WRITE_FPGA 32 $STACKBLK_ADR_LocalIndexReg #x1
    WRITE_FPGA 32 $STACKBLK_ADR_ControlReg $WB_VALUE
    VERIFY_FPGA 32 $STACKBLK_ADR_ControlReg WB_VALUE $WB_EXPECTED_VALUE #xFFFFFFFF
    WRITE_FPGA 32 $STACKBLK_ADR_ControlReg #x0

    -- Wait until operation has been finished
    WAIT_NS 1000

    -- Stack address after current activation frame + 1 local pushed onto stack
    VERIFY_FPGA 32 $STACKBLK_ADR_StackAddressReg WB_VALUE #x18 #xFF

    -- Local 1 on stack
    VERIFY_RAM 32 #x15 WB_VALUE #x44556677 #xFFFFFFFF -- LowValue
    VERIFY_RAM 32 #x16 WB_VALUE #x00000000 #xFFFFFFFF -- HighValue
    VERIFY_RAM 32 #x17 WB_VALUE $STACKBLK_VAL_i32 #xFF -- TypeValue

    -- Local 1 provided to registers
    VERIFY_FPGA 32 $STACKBLK_ADR_LowValueReg WB_VALUE #x44556677 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_HighValueReg WB_VALUE #x0 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_TypeReg WB_VALUE $STACKBLK_VAL_i32 #xFFFFFFFF
    VERIFY_FPGA 32 $STACKBLK_ADR_LocalIndexReg WB_VALUE #x1 #xFFFFFFFF

    RETURN_CALL
END_SUB


